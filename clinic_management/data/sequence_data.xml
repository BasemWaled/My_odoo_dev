<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="seq_clinic_patient" model="ir.sequence">
        <field name="name">Clinic Patient</field>
        <field name="code">clinic.patient</field>
        <field name="prefix">CL</field>
        <field name="padding">5</field>
    </record>
    <record id="seq_clinic_appointment" model="ir.sequence">
            <field name="name">Clinic Appointment</field>
            <field name="code">clinic.appointment</field>
            <field name="prefix">APT</field>
            <field name="padding">0</field>
    </record>
     <record id="clinic_appointments_invoice_sequence" model="ir.sequence">
            <field name="name">clinic Appointments Invoice</field>
            <field name="code">c</field>
            <field name="prefix">MED/INV/</field>
            <field name="padding">4</field>
     </record>
</odoo>